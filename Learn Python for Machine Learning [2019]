 -------------------------------------------------------
 # get rid of repeated numbers in a list or array 
 
 listem = [3, 3, 4, 4, 5, 5, 5, 6, 7, 8, 6, 7, 8, 9]
 s = []
for x in listem:
    if x not in s:
        s.append(x)
print(s)
 s = [3, 4, 5, 6, 7, 8, 9]
 ---------------------------------------------------------
 my_dict = {'name': ["Aziz", "Sinem", "Sebnem", "Bulut"], 'Age': [33, 8, 5, 1]}

my_dict['name'] = ["Aziz", "Sinem", 'Sebnem', "Ozlem"]

my_dict['age'] =  [33, 8, 5, 25]

print(my_dict)

print(my_dict['age'])

my_dict.update({'eye_color': ["Black", "Blue", "Gray", "white"]})

print(my_dict)

print(my_dict.get('name'))

