def test():
    print("I love you")
    print("Do you also love  me")

test()
print("Nevermind")
--------------------------------------------------------------
def great_user(name):
    name = "John"
    print(f'Hi {name}')
    print('Welcome abroad')

print("Start")
great_user("John")
print("Good byeee")

---------------------------------------------------------------
def great_user(first_name, last_name):
    print(f'Hi {first_name} {last_name}')
    print('Welcome abroad')

print("Start")
great_user("Aziz", "Koyuncu")
print("Good byeee")

